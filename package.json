{
  "name": "postcss-example",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "svelte:compiler": {
    "postcss": [
      ["postcss-env-function", {
        "importFrom": {
          "environmentVariables": {
            "--svelte-postcss-color": "rebeccapurple"
          }
        }
      }],
      "postcss-color-gray"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "meteor-node-stubs": "^0.4.1",
    "svelte": "^3.49.0"
  },
  "devDependencies": {
    "postcss-color-gray": "^5.0.0",
    "postcss-env-function": "^2.0.2"
  }
}
